    ENV NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL=${NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL}
    ENV NODE_ENV production
    
    RUN npm run build
    
    # ---- Runner Stage ----
    FROM base AS runner
    WORKDIR /app
    
    ENV NODE_ENV production
    # Runtime Environment Variables (will be set by `docker run -e`)
